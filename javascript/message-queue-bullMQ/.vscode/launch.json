{
  "version": "0.0.1",
  "configurations": [
    {
      "name": "Launch node",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/bin/www",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
    },
    {
      "name": "attach node",
      "type": "node",
      "request": "attach",
      "processId": "${command:PickProcess}",
      "restart": true,
      "internalConsoleOptions": "neverOpen",
    }
  ]
}